{
  "colors": ["#3fb1e3", "#6be6c1", "#626c91", "#a0a7e6", "#c4ebad", "#96dee8"],
  "title": {
    "position":{
      "x":"50%",
      "y":"100%"
    },
    "style": {
      "color": "#444444",
      "fontSize": 14
    }
  },
  "legends":{
    "show":false,
    "style": {
      "color": "#444444",
      "fontSize": 12
    }
  },
  "labels": {
    "style": {
      "color": "#444444",
      "colorReversed": "#ffffff",
      "fontSize": 12
    }
  },
  "axisLine": {
    "style": {
      "color": "#333333"
    }
  },
  "axisTick": {
    "style": {
      "color": "#333333",
      "length": 4
    }
  },
  "splitLine": {
    "style": {
      "color": "#cccccc",
      "dashSize": 5,
      "gapSize": 5
    }
  },
  "plotOptions": {
    "scatter": {
      "radius": 5
    },
    "donut": {
      "clockwise": true,
      "startAngle": 90,
      "innerRadius": "50%",
      "label": {
        "position": "inside"
      }
    },
    "pie": {
      "clockwise": true,
      "startAngle": 90,
      "label": {
        "position": "outside"
      }
    }
  }

}
